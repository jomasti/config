# jumanji config

# search engines
searchengine goog http://www.google.com/search?q=%s
searchengine wiki http://en.wikipedia.org/w/index.php?search=%s
searchengine awiki http://wiki.archlinux.org/index.php?search=%s

# browser settings
set homepage http://home.archlinux.ca

# downloads
set download-dir ~/Downloads
set download-command 'urxvtc -e sh -c "wget --load-cookies /home/josh/.config/jumanji/cookies '%s' -O %s"'

# look
set font "droid sans regular 9"

# key bindings
map <C-q> quit
map J tab_navigate next
map K tab_navigate  previous
map H navigate_history previous
map L navigate_history next

# scripts
#script /home/josh/.config/jumanji/scripts/hinting.js

# adblocking css
#set stylesheet /home/josh/.config/jumanji/fanboy-adblocklist-elements-v4.css
